global:
  reboot:
    min_wait_time: 15
    max_wait_time: 30

  version:
    # https://www.arduino.cc/en/Main/Software
    arduino: "1.8.13"
    # https://golang.org/doc/install
    go: "1.15.1"
    # https://www.jetbrains.com/toolbox-app/download/download-thanks.html
    jetbrains: "1.18.7455"
    # https://github.com/istio/istio/tags
    istioctl: "1.7.0"
    # https://maven.apache.org/download.cgi
    mvn: "3.6.3"
    node: "14.10.1"
    # https://robomongo.org/download
    robo3t: 1.4.0-linux-x86_64-12e54cc

  bastion:
    user: ""
    host: ""
    duo:
      ikey: ""
      skey: ""
      host: ""

  collectd:
    host: ""
    port: "25826"

  docker:
    username: ""
    password: ""
    email: ""

  media:
    nfs:
      plex: ""
      plexlogs: ""
      movies: ""
      tvshows: ""

  nfs:
    host: ""

  plex:
    claim: ""

  sish:
    tag: ""
    sshPort: "8022"
    forwardingPorts:
      http: "80"
      https: "443"
      tcp: []
    domain: ""
    privateKeyPassphrase: ""

  spin_cli:
    gate:
      endpoint: https://spinnaker-api.domain
    auth:
      enabled: true
      oauth2:
        tokenUrl: https://auth.domain/auth
        authUrl: https://auth.domain/token
        clientId: ""
        clientSecret: ""
        scopes:
        - openid
        - email
        - profile
